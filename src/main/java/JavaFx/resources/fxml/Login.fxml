<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JavaFx.Controller.RootController">
   <children>
      <StackPane layoutX="250.0" prefHeight="600.0" prefWidth="550.0" style="-fx-background-color: #2a2e37;">
         <children>
            <JFXButton fx:id="homeBtn" onAction="#goHomeStage" prefHeight="51.0" prefWidth="118.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Go Home" />
         </children>
      </StackPane>
      <VBox prefHeight="600.0" prefWidth="250.0" style="-fx-background-color: #1c1f25; -fx-border-color: #fbf2a2;">
         <children>
            <HBox prefHeight="35.0" prefWidth="250.0">
               <children>
                  <ImageView fx:id="homeBtn" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/home_icon.png" />
                     </image>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <Button mnemonicParsing="false" text="Button" />
               </children>
            </HBox>
            <Rectangle arcHeight="2.0" arcWidth="2.0" fill="#fbf2a2" height="3.0" stroke="BLACK" strokeType="INSIDE" width="250.0" />
            <ImageView fitHeight="100.0" fitWidth="97.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/setting_icon.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="50.0" left="76.0" top="40.0" />
               </VBox.margin>
            </ImageView>
            <Button alignment="CENTER" contentDisplay="CENTER" graphicTextGap="22.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" style="-fx-background-color: #dfd784; -fx-border-color: #fbf2a2; -fx-text-fill: #2a2e37; -fx-font-family: NanumGothic; -fx-font-weight: bold;" text="Settings" textAlignment="CENTER">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </Button>
         </children></VBox> 
   </children>
</AnchorPane>
